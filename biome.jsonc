{
  "formatter": {
    "indentStyle": "space", // default is `tab`
    "lineWidth": 120 // default is `80`
  },
  "javascript": {
    "formatter": {
      "quoteStyle": "single" // default is `double`
    }
  },
  "css": {
    "parser": {
      "cssModules": true
    },
    "formatter": {
      "enabled": true
    }
  }
}
